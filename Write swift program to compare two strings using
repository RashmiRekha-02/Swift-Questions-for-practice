import Foundation

func compareString(str1:String , str2:String) {
  if(str1 == str2)
  {
    print("Both string are equal")
  }
  else if(str1>str2)
  {
    print("\(str1) is larger ")
  }
  else {
    print("\(str2) is larger ")
  }

//way 2

print(str1.compare(str2, options: .caseInsensitive))


}
compareString(str1:"Amit" , str2:"amit")
